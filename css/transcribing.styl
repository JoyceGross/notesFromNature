body.double  header {
  position: fixed;
  top: 0px;
  left: 0;
  width: 100%;
  min-width: 1024px;
  height: 52px;
  z-index: 10;
  -moz-box-shadow: rgba(0, 0, 0, 0.6) 0 0 20px 5px;
  -webkit-box-shadow: rgba(0, 0, 0, 0.6) 0 0 20px 5px;
  -o-box-shadow: rgba(0, 0, 0, 0.6) 0 0 20px 5px;
  box-shadow: rgba(0, 0, 0, 0.6) 0 0 20px 5px;
  background: url("../images/bg.jpg") repeat-x #333333;
  border-bottom: 1px solid rgba(255, 255, 255, 0.02);
}

body.double  header div.wrapper {
  width: 100%;
  height: 80px;
  background-image: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, rgba(0, 0, 0, 0)), color-stop(50%, rgba(0, 0, 0, 0)), color-stop(70%, rgba(0, 0, 0, 0.3)), color-stop(100%, rgba(0, 0, 0, 0.5)));
  background-image: -webkit-linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0) 50%, rgba(0, 0, 0, 0.3) 70%, rgba(0, 0, 0, 0.5));
  background-image: -moz-linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0) 50%, rgba(0, 0, 0, 0.3) 70%, rgba(0, 0, 0, 0.5));
  background-image: -o-linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0) 50%, rgba(0, 0, 0, 0.3) 70%, rgba(0, 0, 0, 0.5));
  background-image: -ms-linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0) 50%, rgba(0, 0, 0, 0.3) 70%, rgba(0, 0, 0, 0.5));
  background-image: linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0) 50%, rgba(0, 0, 0, 0.3) 70%, rgba(0, 0, 0, 0.5));
}
body.double  header div.left {
  float: left;
  height: 60px;
  margin: 0;
  padding: 20px 0 0 24px;
}
body.double  header div.right {
  position: relative;
  float: right;
  height: 55px;
  padding: 25px 24px 0 24px;
}
body.double  header div.right::before {
  position: absolute;
  top: 0;
  left: 1px;
  height: 80px;
  width: 1px;
  content: '';
  border-left: 1px solid rgba(255, 255, 255, 0.1);
}
body.double  header div.right::after {
  position: absolute;
  top: 0;
  left: 0;
  height: 80px;
  width: 1px;
  content: '';
  border-left: 1px solid black;
}
body.double  header h1 {
  margin: 0;
  font: bold 17px "Open Sans";
  color: white;
  font-weight: 800;
}
body.double  header h2 {
  margin: 0;
