.transcriber.birds,
.transcriber.sernac {
  position: absolute;
  top: 0; bottom: 0; right: 0;
  left: 0;
  overflow: hidden;
  background: url("../images/transcribers/bg-transcriber.png");
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  -webkit-user-drag: none;
}

.transcriber.birds .tooltip,
.transcriber.sernac .tooltip {
  display: none;
  position: absolute;
  width: 175px;
  right: 0;
  padding: 12px 12px;
  background: white;
  text-align: center;
  -webkit-border-radius: 2px;
  -moz-border-radius: 2px;
  -ms-border-radius: 2px;
  -o-border-radius: 2px;
  border-radius: 2px;
  -webkit-box-shadow: rgba(0, 0, 0, 0.2) 0 0 3px 2px;
  -moz-box-shadow: rgba(0, 0, 0, 0.2) 0 0 3px 2px;
  box-shadow: rgba(0, 0, 0, 0.2) 0 0 3px 2px;
  z-index: 10010;
}
.transcriber.birds .tooltip img,
.transcriber.sernac .tooltip img {
  margin: 0 0 3px 0;
  display: none;
}
.transcriber.birds .tooltip.tooltip.example,
.transcriber.sernac .tooltip.tooltip.example {
  height: 150px;
}
.transcriber.birds .tooltip.tooltip.example .loader,
.transcriber.sernac .tooltip.tooltip.example .loader {
  top: 80px;
}
.transcriber.birds .tooltip.tooltip.example .btn,
.transcriber.sernac .tooltip.tooltip.example .btn {
  position: absolute;
  bottom: 14px;
  left: 50%;
  width: 80px;
  margin-left: -40px;
}
.transcriber.birds .tooltip.with-spinner,
.transcriber.sernac .tooltip.with-spinner {
  width: 200px;
  height: 160px;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
}
.transcriber.birds .tooltip.with-spinner img,
.transcriber.sernac .tooltip.with-spinner img {
  position: absolute;
  top: 10px;
  width: 180px;
  height: 100px;
  margin-left: -90px;
  left: 50%;
}
.transcriber.birds .tooltip.with-spinner .btn,
.transcriber.sernac .tooltip.with-spinner .btn {
  position: absolute;
  bottom: 10px;
  left: 50%;
  width: 80px;
  margin-left: -40px;
}
.transcriber.birds .tooltip.with-spinner .loader,
.transcriber.sernac .tooltip.with-spinner .loader {
  top: 30px;
  margin-top: 0;
}
.transcriber.birds .tooltip .loader,
.transcriber.sernac .tooltip .loader {
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -30px;
  margin-left: -30px;
  border: none;
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
}
.transcriber.birds .tooltip .tip,
.transcriber.sernac .tooltip .tip {
  position: absolute;
  bottom: -6px;
  left: 50%;
  margin-left: -6px;
  width: 13px;
  height: 6px;
  background: url(../images/transcribers/tooltip-tip.png) no-repeat;
}
.transcriber.birds .tooltip .title,
.transcriber.sernac .tooltip .title {
  font: bold 11px "Open Sans";
  color: #191919;
  text-transform: uppercase;
}
.transcriber.birds .tooltip .description,
.transcriber.sernac .tooltip .description {
  padding: 4px 0 0 0;
  margin: 0 0 10px 0;
  font: normal 13px "Helvetica",Arial;
  color: #666;
}
.transcriber.birds .tooltip .description a,
.transcriber.sernac .tooltip .description a {
  color: #666;
}
.transcriber.birds .tooltip .btn,
.transcriber.sernac .tooltip .btn {
  padding: 5px 0;
  width: 83px;
  font-family: "Open Sans", "Helvetica", sans-serif;
  font-weight: 800;
  -webkit-transition-property: background-color;
  -moz-transition-property: background-color;
  -o-transition-property: background-color;
  transition-property: background-color;
  -webkit-transition-duration: 300ms;
  -moz-transition-duration: 300ms;
  -o-transition-duration: 300ms;
  transition-duration: 300ms;
}
.transcriber.birds .tooltip .btn.main,
.transcriber.sernac .tooltip .btn.main {
  float: none;
  background: #F60;
  color: #FFF;
}
.transcriber.birds .tooltip .btn.secondary,
.transcriber.sernac .tooltip .btn.secondary {
  float: none;
  background: #f1f1f1;
  color: #666;
  border: 1px solid #ccc;
}
.transcriber.birds .tooltip .btn.main:hover,
.transcriber.sernac .tooltip .btn.main:hover {
  background: #FF7C09;
}
.transcriber.birds .tooltip .btn.secondary:hover,
.transcriber.sernac .tooltip .btn.secondary:hover {
  background: #fff;
}
.transcriber.birds .statusbar,
.transcriber.sernac .statusbar {

  h1, h3 { margin: 0; }

  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  min-width: 1024px;
  height: 80px;

  -webkit-box-shadow: 0 0 5px 5px rgba(0, 0, 0, 0.6);
  -moz-box-shadow: 0 0 5px 5px rgba(0, 0, 0, 0.6);
  -o-box-shadow: 0 0 5px 5px rgba(0, 0, 0, 0.6);
  box-shadow: 0 0 5px 5px rgba(0, 0, 0, 0.6);

  background: #222222 url("../images/transcribers/bg-header.png");
  border-bottom: 1px solid rgba(255, 255, 255, 0.02);

  z-index: 101;
}

.transcriber.birds .statusbar .left,
.transcriber.sernac .statusbar .left {
  float: left;
  height: 60px;
  margin: 0;
  padding: 20px 0 0 24px;
}
.transcriber.birds .statusbar .right,
.transcriber.sernac .statusbar .right {
  position: relative;
  float: right;
  height: 55px;
  width: 90px;
  padding: 25px 24px 0 24px;
}
.transcriber.birds .statusbar .right::before,
.transcriber.sernac .statusbar .right::before {
  position: absolute;
  top: 0;
  left: 1px;
  height: 80px;
  width: 1px;
  content: '';
  border-left: 1px solid rgba(255, 255, 255, 0.1);
}
.transcriber.birds .statusbar .right::after,
.transcriber.sernac .statusbar .right::after {
  position: absolute;
  top: 0;
  left: 0;
  height: 80px;
  width: 1px;
  content: '';
  border-left: 1px solid black;
}
.transcriber.birds .statusbar h1,
.transcriber.sernac .statusbar h1 {
  font: bold 17px "Open Sans";
  color: white;
  font-weight: 800;
  text-transform: uppercase;
}
.transcriber.birds .statusbar h3,
.transcriber.sernac .statusbar h3 {
  font: normal 13px "Open Sans";
  color: #999;
}
.transcriber.birds .statusbar .counter,
.transcriber.sernac .statusbar .counter {
  position: absolute;
  top: 10px;
  font: bold 38px "Open Sans";
  color: white;
  font-weight: 800;
}
.transcriber.birds .statusbar strong,
.transcriber.sernac .statusbar strong {
  float: left;
  width: 50px;
  line-height: 0;
  position: absolute;
  top: 23px;
  left: 55px;
  line-height: 15px!important;
  font: bold 12px "Open Sans";
  color: white;
  font-weight: 800;
  text-transform: uppercase;
}
.transcriber.birds .photos,
.transcriber.sernac .photos {
  margin: auto;
  text-align: center;
  overflow: auto;
  height: 100%;
  z-index: 10;
}
.transcriber.birds .photos.selectable img,
.transcriber.sernac .photos.selectable img {
  cursor: crosshair;
}


.transcriber.sernac .photos img {
  display: none;
  margin: 120px auto 20px auto;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  -webkit-user-drag: none;
  -webkit-box-shadow: 1px 1px 9px rgba(0,0,0,.7);
  -moz-box-shadow:    1px 1px 9px rgba(0,0,0,.7);
  -o-box-shadow:      1px 1px 9px rgba(0,0,0,.7);
  box-shadow:         1px 1px 9px rgba(0,0,0,.7);
}

.transcriber.birds .photos img {
  display: none;
  margin: 120px auto 20px auto;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  -webkit-user-drag: none;
