  -moz-box-shadow: 0 0 15px 13px rgba(0, 0, 0, 0.7);
  box-shadow: 0 0 15px 13px rgba(0, 0, 0, 0.7);
}

.transcriber.birds .bar,
.transcriber.sernac .bar {
  display: none;
  position: absolute;
  width: 520px;
  height: 55px;
