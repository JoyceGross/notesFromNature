.mCSB_scrollTools .mCSB_buttonDown {
  height: 20px;
  overflow: hidden;
  margin: 0 auto;
  cursor: pointer;
}

.mCSB_scrollTools .mCSB_buttonDown {
  bottom: 0;
  margin-top: -40px;
}

