.ui-resizable-n  { cursor: n-resize;  @include size(100%, 7px); @include position(-5px, false, false, 0); }
.ui-resizable-s  { cursor: s-resize;  @include size(100%, 7px); @include position(false, false, -5px, 0); }
.ui-resizable-e  { cursor: e-resize;  @include size(7px, 100%); @include position(0, -5px, false, false); }
.ui-resizable-w  { cursor: w-resize;  @include size(7px, 100%); @include position(0, false, false, -5px); }
.ui-resizable-se { cursor: se-resize; @include size(12px);      @include position(false, 1px, 1px, false); }
