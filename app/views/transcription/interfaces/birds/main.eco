<section class="menu">
  <div class="left">
    <div class="block">
      <h1><%- @archive.name %></h1>
      <p>from <%- @archive.institute().name %></p>
    </div>
    <div class="block">
      <button id="finish">Finish</button>
    </div>
  </div>
  <div class="right">
    <div class="block settings options" id="settingsBlock">
      <button></button>
      <ul>
        <li>
          <label><input type="checkbox" id="autoMove" <% if @preferences.auto_move: %> checked<% end %>>
          Automatically move to next entity</label>
        </li>
      </ul>
    </div>
    <div class="block tools options">
      <button></button>
      <ul id="tools-list">
        <li id="cursor">Cursor</li>
        <li id="multi-select">Multi-Select</li>
      </ul>
    </div>
    <div class="block"><a href="#/archives/nhm_birds"><div class="ring"><span></span></div></a></div>
  </div>
</section>

<div class="boxes">
  <img src="images/ocr/AV16008.JPG"></img>
</div>

<div id="data-entry" class="data-entry">
  <section class="entry" id="entry"></section>
  <section class="actions">
    <h2>Actions</h2>
    <ul id="actions"></ul>
</section>

</div>