
<script type="text/template" id="transcriber-birds-widget-template">

  <div class="top">
    <div class="title">Welcome</div>
  </div>

  <div class="bottom">
    <div class="left">

      <div class="input_field hidden">
        <input type="text" placeholder="">
        <a href="#" class="btn ok">ok</a>
      </div>

      <div class="description">Drag & resize the viewer to the record you want to transcribe.</div>
    </div>

    <div class="right">
      <div class="step">1/2</div>
      <a href="#" class="btn start">Start this page</a>
      <a href="#" class="btn finish">Finish this page</a>
      <a href="#" class="btn nextLine">Next Line</a>
    </div>
  </div>

</script>

<script type="text/template" id="tooltip-step-template">
  <ul>
    {{#links}}
    <li><a href="#">{{title}}</a></li>
    {{/links}}
  </ul>
  <div class="tip"></div>
</script>

<script type="text/template" id="tooltip-template">
  <strong class="title">{{ title }}</strong>
  <div class="description">{{{ description }}}</div>

  <div class="buttons">
    <a href="#" class="btn main">{{ main }}</a> 
    <a href="#" class="btn secondary">{{ secondary }}</a>
  </div>

  <div class="tip"></div>
</script>

<script type="text/template" id="tooltip-example-template">
  <div class="buttons"><a href="#" class="btn main">{{ main }}</a></div> 
  <div class="tip"></div>
</script>

<script type="text/template" id="statusbar-template">
  <div class="left">
    <h1 class="title">{{ title }}</h1>
    <h3 class="description">{{ description }}</h3>
  </div>
  <div class="right">
    <div class="separator"></div>

    <div class="count">
      <div class="counter">0</div>
      <strong>Records done</strong>
    </div>

    <div class="separator"></div>

    <div class="off">
      <a href="/" class="btn close"></a>
    </div>
  </div>
</script>

<script type="text/template" id="date-input-template">
  <div class="date_field"><input type="text" name="month" class="month" placeholder="month" /></div> 
  <div class="date_field"><input type="text" name="day"   class="day"   placeholder="day" /></div> 
  <div class="date_field"><input type="text" name="year"  class="year"  placeholder="year" /></div>
</script>

<script type="text/template" id="extra-input-template">
  <div class="extra_field"><input type="text" name="gender"   class="gender"   placeholder="gender" /></div> 
  <div class="extra_field"><input type="text" name="age"      class="age"      placeholder="age" /></div> 
  <div class="extra_field"><input type="text" name="register" class="register" placeholder="register" /></div>
</script>

<script type="text/template" id="tooltip-template">
  <strong class="title">{{ title }}</strong>
  <div class="description">{{{ description }}}</div>

  <div class="buttons">
    <a href="#" class="btn main">{{ main }}</a> 
    <a href="#" class="btn secondary">{{ secondary }}</a>
  </div>

  <div class="tip"></div>
</script>

<script type="text/template" id="tooltip-example-template">
  <div class="buttons"><a href="#" class="btn main">{{ main }}</a></div> 
  <div class="tip"></div>
</script>
