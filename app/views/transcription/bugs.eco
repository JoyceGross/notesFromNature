<script type="text/template" id="county-input-template">
  <div class="county_field">
    <select name="county" class="county">
      {{#counties}}
      <option value="{{.}}">{{.}}</option>
      {{/counties}}
    </select>
  </div>
</script>

<script type="text/template" id="latlng-input-template">
  <div class="latlng_field lat"><input type="text" name="lat" class="lat" placeholder="latitude"></div>
  <div class="latlng_field lng"><input type="text" name="lng" class="lng" placeholder="longitude"></div>
</script>

<script type="text/template" id="tooltip-error-template">
  <strong class="title">{{title}}</strong>
  <div class="description">{{description}}</div>
  <div class="tip"></div>
</script>

<script type="text/template" id="tooltip-step-template">
  <ul>
    {{#links}}
    <li><a href="#">{{title}}</a><span></span></li>
    {{/links}}
  </ul>
  <div class="tip"></div>
</script>

<script type="text/template" id="tooltip-template">
  <strong class="title">{{{ title }}}</strong>
  <div class="description">{{{ description }}}</div>

  <div class="buttons">
    <a href="#" class="btn main">{{ main }}</a> 
    <a href="#" class="btn secondary">{{ secondary }}</a>
  </div>

  <div class="tip"></div>
</script>

<script type="text/template" id="tooltip-example-template">
  <div class="buttons"><a href="#/archives/Herbarium" class="btn main">{{ main }}</a></div> 
  <div class="tip"></div>
</script>

<script type="text/template" id="statusbar-template">
  <div class="left">
    <h1 class="title">{{ title }}</h1>
    <h3 class="description">{{ description }}</h3>
  </div>
  <div class="right">

    <div class="separator"></div>

    <div class="count">
      <div class="counter">0</div>
      <strong class="counter-text">Records done</strong>
    </div>

    <div class="separator"></div>

    <div class="off">
      <a href="/" class="btn close"></a>
    </div>

  </div>
</script>
