
      
      <div class="box-general museum">
        <div class="col3">
          <h2 class="title">
            <%=@institute.name%>
            <span class="subtitle"><%=@institute.location%></span>
            <img class="logo" src="/images/logoMuseum.png" alt="Natural History Museum" />
          </h2>
        </div>
      </div>          

      <div class="box-general divided">
      
        <div class="col2">
          <div>
            <p><%-@institute.description%></p>
          </div>

        </div>
        <div class="col1">
          <div class="links-left">            
            <span class="text">MORE INFORMATION</span>
            <a href="<%=@institute.url%>" class="object noline"><%=@institute.url%></a>
            <span class="text">MORE INFORMATION</span>
            <ul>
              <% for insitite in @institute.insitites().all(): %>
                <li class="bullet"><a href="" class="object noline"><%=insitite.name%></a></li>
              <%end%>
              
            </ul>       
          </div>
        </div>
        
      </div>
      
      
      <% for insitite in @institute.insitites().all(): %>
        <%- @insititeTemplate(insitite)%>
      <%end%>    
    
      <div class="box-general divided-bg">
      
        <div class="col2">
          <div>
